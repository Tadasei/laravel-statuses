{
    "name": "tadasei/laravel-statuses",
    "description": "A package that provides stubs for managing status associations with models",
	"keywords": ["laravel", "status", "stubs", "backend"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tadasei",
            "email": "chemselhak.mahouche@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/filesystem": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/validation": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpstan/phpstan": "^1.10",
        "symfony/var-dumper": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Tadasei\\Statuses\\": "src/"
        }
    },
	"config": {
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"Tadasei\\Statuses\\ServiceProvider"
			]
		}
	}
}
